{
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "../../.moon/cache/types/apps/svc-users",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@nekotoko/shared-nest-utils": [
        "../../libs/shared-nest-utils/src/index.ts"
      ],
      "@nekotoko/shared-nest-utils/*": [
        "../../libs/shared-nest-utils/src/*"
      ]
    },
    "plugins": [
      {
        "transform": "@nestia/core/lib/transform",
        "validate": "assert",
        "stringify": "assert"
      },
      {
        "transform": "typia/lib/transform"
      }
    ]
  },
  "exclude": [
    "./dist",
    "./scripts",
    "./src/generated",
    "jest.config.ts",
    "**/*.spec.ts",
    "**/*.test.ts"
  ],
  "references": [
    {
      "path": "../../libs/shared-nest-utils"
    }
  ]
}
