{
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "../../.moon/cache/types/apps/svc-users",
    "removeComments": true,
    "esModuleInterop": true,
    "emitDeclarationOnly": false,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@nekotoko/shared-nest-utils": [
        "../../libs/shared-nest-utils/src/index.ts"
      ],
      "@nekotoko/shared-nest-utils/*": [
        "../../libs/shared-nest-utils/src/*"
      ]
    },
    "plugins": [
      {
        "transform": "@nestia/core/lib/transform",
        "validate": "assert",
        "stringify": "assert"
      },
      {
        "transform": "typia/lib/transform"
      }
    ]
  },
  "references": [
    {
      "path": "../../libs/shared-nest-utils"
    }
  ],
  "exclude": [
    "node_modules",
    "dist",
    "scripts"
  ]
}
